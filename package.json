{
  "name": "react-hover-flip",
  "version": "0.4.0",
  "description": "A react component that will flip on hover",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "repository": {
    "type": "git",
